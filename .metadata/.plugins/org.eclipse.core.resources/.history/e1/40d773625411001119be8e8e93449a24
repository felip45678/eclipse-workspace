package repaso;

import java.util.Scanner;

public class Palabra {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String palabra = "";
		for(int i=0;i<palabra.split(" ").length;i++) {
			if(palabra.split(" ")[i]!="")
				System.out.println("palabra "+palabra.split(" "));
		}
		sc.close();

	}

}
