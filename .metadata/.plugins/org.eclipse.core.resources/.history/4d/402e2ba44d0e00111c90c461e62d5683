package colecciones;

import java.util.HashSet;
import java.util.Set;

public class Set01 {

	public static void main(String[] args) {
		// creamos un conjunto de numeros eneteros 
		Set<Integer> numberSet=new HashSet<Integer>();
		
		//insertamos 6 elementos en el conjunto 
		numberSet.add(1);
		numberSet.add(3);
		numberSet.add(2);
		numberSet.add(1);
		numberSet.add(11);
		numberSet.add(2);

		System.out.println("Conjuto: "+numberSet.toString());
		System.out.println("Tama√±o: "+numberSet.size());
		
		if(!numberSet.add(3))
			System.out.println("Error al insertar 3");
		if(numberSet.contains(3))
			System.out.println("El valor 3 ya esta en el conjunto");
	}

}
